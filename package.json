{
  "name": "trtplusstoolbox",
  "description": "A Vue 3 application for TRT+ Toolbox",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build-bulma": "sass --load-path=node_modules src/assets/main.scss src/assets/main.css",
    "start": "npm run build-bulma -- --watch",
    "dev": "concurrently \"npm run build-bulma -- --watch\" \"vite\"",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "bulma": "^1.0.4",
    "material-symbols": "^0.31.4",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "concurrently": "^9.1.2",
    "sass": "^1.89.1",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2"
  }
}
